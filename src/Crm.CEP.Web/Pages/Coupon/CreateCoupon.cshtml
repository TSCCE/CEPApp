@page
@model Crm.CEP.Web.Pages.Coupon.CreateCouponModel
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@{
    Layout = null;
}

<abp-dynamic-form abp-model="Coupon" asp-page="/Coupon/CreateCoupon">
    <abp-modal>
        <abp-modal-header title="New Coupon"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>